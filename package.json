{
  "name": "@tableland/sdk",
  "version": "4.0.0",
  "description": "A D1Database client and helpers for the @tableland network",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./helpers": {
      "require": "./dist/helpers/index.js",
      "import": "./dist/helpers/index.js",
      "default": "./dist/helpers/index.js"
    },
    "./registry": {
      "require": "./dist/registry/index.js",
      "import": "./dist/registry/index.js",
      "default": "./dist/registry/index.js"
    },
    "./validator": {
      "require": "./dist/validator/index.js",
      "import": "./dist/validator/index.js",
      "default": "./dist/validator/index.js"
    },
    "./database": {
      "require": "./dist/database.js",
      "import": "./dist/database.js",
      "default": "./dist/database.js"
    },
    "./statement": {
      "require": "./dist/statement.js",
      "import": "./dist/statement.js",
      "default": "./dist/statement.js"
    },
    "./**/*.js": {
      "require": "./dist/*",
      "import": "./dist/*",
      "default": "./dist/*"
    }
  },
  "files": [
    "dist/**/*.js?(.map)",
    "dist/**/*.d.ts",
    "src/**/*.ts"
  ],
  "scripts": {
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{ts,json,sol,md}' --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "coverage": "c8 mocha --exit",
    "clean": "rm -rf dist",
    "build:api": "npx openapi-typescript https://raw.githubusercontent.com/tablelandnetwork/docs/carson/429-code/specs/validator/tableland-openapi-spec.yaml --output src/validator/client/validator.ts --immutable-types --path-params-as-types",
    "build:ts": "npx tsc",
    "build": "npm run build:api && npm run build:ts"
  },
  "keywords": [
    "tableland",
    "sql",
    "ethereum",
    "database"
  ],
  "license": "MIT AND Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@databases/split-sql-query": "^1.0.3",
    "@databases/sql": "^3.2.0",
    "@ethersproject/experimental": "^5.7.0",
    "@tableland/local": "^0.0.4",
    "@tableland/sdk": "^3.1.5",
    "@types/assert": "^1.5.6",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "assert": "^2.0.0",
    "c8": "^7.12.0",
    "camelize-ts": "^2.1.1",
    "d1-orm": "^0.7.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.1.0",
    "openapi-typescript": "^6.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@tableland/evm": "^3.0.3",
    "@tableland/sqlparser": "tablelandnetwork/wasm-sqlparser",
    "ethers": "^5.7.2"
  }
}
